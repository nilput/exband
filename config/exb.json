{
    "http" : {
        "listen" : "8085",
        "rules" : [
            {
                
                "type": "prefix",
                "prefix": "/ww",
                "destination": {
                    "type": "handler",
                    "name": "basic_handle_request"
                }
            }
        ]
    },
    "event" : {
        "processes" : 0,
        "loops" : 4,
        "polling" : "epoll",
        "aio" : false,
        "threadpool": 0
    },
    "modules": [
        {
            "path": "examples/basic/basic.so:handler",
            "args" : {
                "db_path" : "./db.sqlite3",
                "db_reinit" : 0,
                "site_link" : "http://127.0.0.1:8085"
            },
            "import" : "basic_handle_request"
        }
    ]
}
